{
  "_version": "1.1.0",
  "sap.app": {
	"_version": "1.1.0",
	
	"dataSources": {
	  "invoiceRemote": {
		"uri": "http://bcsw-sap078.mymhp.net:8000/sap/opu/odata/SAP/ZSMT1ODATA_SRV/",
		"type": "OData",
		"settings": {
		  "odataVersion": "2.0"
		}
	  }
	},
	"id": "zsmt1",
	"type": "application",
	"i18n": "i18n/i18n.properties",
	"title": "{{appTitle}}",
	"description": "{{appDescription}}",
	"applicationVersion": {
	  "version": "1.0.0"
	}
  },
  "sap.ui": {
	"_version": "1.1.0",
	"technology": "UI5",
	"deviceTypes": {
	  "desktop": true,
	  "tablet": true,
	  "phone": true
	},
	"supportedThemes": [
		"sap_hcb",
		"sap_belize"
	]
  },
  "sap.ui5": {
	"_version": "1.1.0",
	"rootView": "zsmt1.view.App",
	"dependencies": {
	  "minUI5Version": "1.30",
	  "libs": {
		"sap.m": {}
	  }
	},
	"models": {
	  "i18n": {
		"type": "sap.ui.model.resource.ResourceModel",
		"settings": {
		  "bundleName": "zsmt1.i18n.i18n"
		}
	  },
	  "": {
	    "dataSource":"invoiceRemote"
	  }
	},
	"routing": {
	  "config": {
		"routerClass": "sap.m.routing.Router",
		"viewType": "XML",
		"viewPath": "zsmt1.view",
		"controlId": "app",
		"controlAggregation": "pages",
		"bypassed": {
               "target": "notFound"
            }
	  },
	  "routes": [
		{
		  "pattern": "",
		  "name": "App",
		  "target": "App"
		},
		{
		  "pattern": "login/{isManager}",
		  "name": "login",
		  "target": "login"
		},
		{
		  "pattern": "employee/{employeeId}",
		  "name": "employee",
		  "target": "employee"
		},
		{
		"pattern":"manager",
		"name":"manager",
		"target":"manager"
		}
	  ],
	  "targets": {
		"App": {
		  "viewName": "App",
		  "viewLevel":1
		},
		"login": {
		  "viewName": "Login",
		  "transition": "flip",
		  "viewLevel":2
		},
		"employee":
		{
			"viewName":"Employee",
			"viewLevel":3
			
		},
		"manager":
		{
			"viewName":"Manager",
			"viewLevel":3
		},
            "notFound": {
               "viewName": "NotFound",
               "transition": "show"
            }
	  }
	},
	"resources": {
	  "css": [
		{
		  "uri": "css/style.css"
		}
	  ]
	}
  }
}
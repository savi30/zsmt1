<mvc:View 
	controllerName="zsmt1.controller.EmployeeAllColleagues" 
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page title="{i18n>appTitle}"		  
		  class="sapUiResponsiveContentPadding myAllPages" showNavButton="true"
		navButtonPress="onNavBack">
	<headerContent>
		<Toolbar id="employeeAllColleaguesToolBar">
				<Button  icon="sap-icon://home" press="onNavBack"
				class="sapUiSmallMarginBottom" />
				<ToolbarSpacer/>
				<Button icon="sap-icon://menu2" press="handleOpenToolbar"
				class="sapUiSmallMarginBottom"/>
		</Toolbar>
	</headerContent>
	
	<Panel id="employeeAllColleaguesDetailsFragment"/>
	
	<mvc:XMLView viewName="zsmt1.view.EmployeeAllColleaguesTabBar"/> 
	
	</Page>
</mvc:View>
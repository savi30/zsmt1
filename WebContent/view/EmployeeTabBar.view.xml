<mvc:View 
	controllerName="zsmt1.controller.EmployeeTabBar" 
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	
	<IconTabBar
				id="iconTabBar"
				class="sapUiResponsiveContentPadding">
				<items>
					<IconTabFilter id="projectsTab" text="{i18n>projectListTitle}" key="Projects">
						<mvc:XMLView viewName="zsmt1.view.EmployeeProjects"/> 
					</IconTabFilter>
					<IconTabFilter id="skillsTab" text="{i18n>skillListTitle}" key="Skills">
						<mvc:XMLView viewName="zsmt1.view.EmployeeSkills"/> 
					</IconTabFilter>
					<IconTabFilter id="employeeAllProjects" text="{i18n>projectAllListTitle}" key="AllProjects">
						<mvc:XMLView viewName="zsmt1.view.EmployeeAllProjects"/> 
					</IconTabFilter>										
					
				</items>
			</IconTabBar>
	
</mvc:View>
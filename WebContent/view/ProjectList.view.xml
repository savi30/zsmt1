<mvc:View controllerName="zsmt1.controller.ProjectList" xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<List id="projectList" class="sapUiResponsiveMargin" width="auto"
		items="{
					path:'/ProjectSet',
					sorter : {
							path : 'CustName',
							group : true
						}
					}">
		<headerToolbar>
			<Toolbar>
				<Title text="{i18n>projectListTitle}" />
				<ToolbarSpacer />
			</Toolbar>
		</headerToolbar>
		<items>
			<ObjectListItem title="{Name}" type="Navigation"
				press="onPress">
			</ObjectListItem>
		</items>
	</List>
</mvc:View>
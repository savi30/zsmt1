<mvc:View 
	controllerName="zsmt1.controller.ManagerEmployee" 
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page title="{i18n>appTitle}"		  
		  class="sapUiResponsiveContentPadding">
	<headerContent>
		<Toolbar id="managerEmployeeToolBar">
				<ToolbarSpacer/>
				<Button icon="sap-icon://menu2" press="handleOpenToolbar"
				class="sapUiSmallMarginBottom"/>
		</Toolbar>
	</headerContent>
	
	<Panel id="managerEmployeeDetailsFragment"/>
	
	<mvc:XMLView viewName="zsmt1.view.ManagerEmployeeTabBar"/> 
	
	</Page>
</mvc:View>
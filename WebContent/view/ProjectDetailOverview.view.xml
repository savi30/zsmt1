<mvc:View 
	controllerName="zsmt1.controller.Project" 
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page  title="{i18n>appTitle}"		  
		   class="sapUiResponsiveContentPadding myAllPages"
		   showNavButton="true"
		   navButtonPress="onNavBack" >

	<mvc:XMLView viewName="zsmt1.view.EmployeeProjectDetails"/> 
	
	<IconTabBar id="iconTabBarProjDetail"
				class="sapUiResponsiveContentPadding">
	<items>
	<IconTabFilter id="employeeTabManager" text="{i18n>selectEmployeeText}" key="Employee">
						<mvc:XMLView viewName="zsmt1.view.EmployeeProjectEmployee"/> 
	</IconTabFilter>
	</items>
	</IconTabBar>
	
	
	</Page>
</mvc:View>
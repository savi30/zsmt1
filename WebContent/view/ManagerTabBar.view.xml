<mvc:View
	controllerName="zsmt1.controller.Manager"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form"
	busyIndicatorDelay="0">
	<App>
	<Page
		title="{i18n>appTitle}"
		id="managerPage"
		showNavButton="true"
		navButtonPress="onNavBack"
		class="sapUiResponsiveContentPadding">
		<content>
			<IconTabBar
				id="iconTabBar"
				class="sapUiResponsiveContentPadding"
				binding="{Manager}">
				<items>	
					<IconTabFilter id="empTab" text="{i18n>employeeListTitle}" key="Employee">
					</IconTabFilter>
				
					<IconTabFilter id="projectTab" text="{i18n>projectListTitle}" key="Projects">
						<mvc:XMLView viewName="zsmt1.view.ProjectList"></mvc:XMLView>					
					</IconTabFilter>
					
					<IconTabFilter id="reportTab" text="{i18n>reportTitle}" key="Reports">
					</IconTabFilter>
					

<!--  doar pentru a vedea ca merge si lista de skill 
					
					<IconTabFilter id="skillTab" text="{i18n>skillListTitle}" key="Skills"> 
 						<mvc:XMLView viewName="zsmt1.view.SkillList"></mvc:XMLView>	 
					</IconTabFilter>
					 -->
					 
					 
				</items>
			</IconTabBar>
		</content>
	</Page>
	</App>
</mvc:View>
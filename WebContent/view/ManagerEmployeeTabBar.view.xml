<mvc:View 
	controllerName="zsmt1.controller.EmployeeTabBar" 
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	
	<IconTabBar
				id="iconTabBarManager"
				class="sapUiResponsiveContentPadding">
				<items>
					<IconTabFilter id="projectsTabManager" text="Projects" key="Projects">
						<mvc:XMLView viewName="zsmt1.view.ManagerEmployeeProjects"/> 
					</IconTabFilter>
					<IconTabFilter id="skillsTabManager" text="Skills" key="Skills">
						<mvc:XMLView viewName="zsmt1.view.ManagerEmployeeSkills"/> 
					</IconTabFilter>
				</items>
			</IconTabBar>
	
</mvc:View>